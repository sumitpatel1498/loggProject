
@{
    ViewBag.Title = "Index";
}
@section HeaderSection {
    @Styles.Render("~/Content/Student")
}
<h2>Index</h2>
<div ng-app="studentApp" ng-controller="PathwayCtrl as sp">
    <div>
        <button class="btnColor">
            Red
        </button><br /><br />

        <br /><br />
    </div>
    {{firstName}}<br /><br />
    {{sp.firstName}}<br /><br />
    <div>
        <md-datepicker ng-model="sp.myDate" md-placeholder="Enter date"
                       input-aria-describedby="datepicker-description"
                       input-aria-labelledby="datepicker-header "></md-datepicker>
        <p style="display: none" id="datepicker-description">
    </div>
</div>
    @section scripts{
        @Scripts.Render("~/bundles/Angular")
        @Scripts.Render("~/bundles/Common")
        @Scripts.Render("~/bundle/Student")
    }
